<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<appSettings>
		<!-- Semicolon-separated list of Mono runtime path prefixes. -->
		<add key="runtimePathPrefixes" value="" />
		<!-- Semicolon-separated list of fully-qualified paths to addin assemblies. -->
		<add key="addinAssemblyPaths" value="" />
		<!-- Boolean indicating whether or not to use console colors. -->
		<add key="useColors" value="True" />
		<add key="infoColor" value="White" />
		<add key="emphasisColor" value="Cyan" />
		<add key="warningColor" value="Yellow" />
		<add key="errorColor" value="Red" />
		<add key="debugColor" value="Magenta" />
	</appSettings>
</configuration>
